{
    "private": true,
    "type": "module",
    "scripts": {
        "build": "rollup -c",
        "manual": "jsdoc -c ./jsdoc.json",
        "doc1": "jsdoc -c ./doc/.defs/jsdoc1.json",
        "doc2": "jsdoc -c ./doc/.defs/jsdoc2.json",
        "doc3": "jsdoc -c ./doc/.defs/jsdoc3.json",
        "doc": "npm run doc1 && npm run doc2 && npm run doc3"
    },
    "devDependencies": {
        "rollup-plugin-sourcemaps": "^0.6.3",
        "rollup-plugin-terser": "^7.0.2"
    }
}